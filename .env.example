LOCAL_NODE_PRIVATE_KEY=0x105d050185ccb907fba04dd92d8de9e32c18305e097ab41dadda21489a211524
LOCAL_NODE_ADDRESS=0x67D8d32E9Bf1a9968a5ff53B87d777Aa8EBBEe69

# Mirror Node endpoints (Local Node)
MIRROR_NODE_REST_URL=http://127.0.0.1:5551
